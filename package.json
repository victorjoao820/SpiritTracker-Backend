{
  "name": "spirit-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Spirit Co Tracker",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/server.ts",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "prisma migrate dev --schema ./prisma --name dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node src/seed.js",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy"
  },
  "keywords": [
    "spirit",
    "tracker",
    "api",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "prisma": "^6.16.1"

  },
  "prisma": {
    "schema": "./prisma"
  }
}
